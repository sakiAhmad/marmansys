
<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>Add Company</title>
    </head>
    <body>
    <center>
        <form>
            <h1>Add Company</h1>
            <table>
                <tr>
                    <td>Company Name :</td><td><input type="text" id="companyName"/></td>
                </tr>
                <tr>
                    <td>Company Address :</td><td><input type="text" id="companyAddress"/></td>
                </tr>
                <tr>
                    <td>Company Email :</td><td><input type="text" id="companyEmail"/></td>
                </tr>
                <tr>
                    <td>Company Phone :</td><td><input type="text" id="companyPhone"/></td>
                </tr>
                <tr>
                    <td>Company Mobile :</td><td><input type="text" id="companyMobile"/></td>
                </tr>
                <tr>
                    <td><input type="button" value="Submit" id="submit"/></td><td><input type="button" value="Reset" id="reset"/></td>
                </tr>
            </table>
        </form>
    </center>

</body>
</html>
